{
  "name": "tealim",
  "version": "1.0.3",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "dev": "expo start --dev-client",
    "web": "yarn sb:update && expo start --web",
    "eject": "expo eject",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "dist": "eas build --profile production --platform android",
    "sb:update": "sb-rn-get-stories --config-path .ondevice",
    "sb:web": "yarn sb:update && start-storybook -p 6006",
    "sb:build": "yarn tailwind && build-storybook",
    "sb:generate": "sb-rn-get-stories --config-path .ondevice",
    "sb:watch": "sb-rn-watcher --config-path .ondevice",
    "sb": "sb-rn-get-stories --config-path .ondevice expo start",
    "sb:ios": "sb-rn-get-stories --config-path .ondevice expo start --ios",
    "sb:android": "sb-rn-get-stories --config-path .ondevice  expo start --android",
    "test": "jest --watchAll",
    "tailwind": "npx tailwind -i ./styles.css -o ./dist/styles/tailwind.css",
    "tailwind:watch": "npx tailwind -i ./styles.css -o ./dist/styles/tailwind.css --watch"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-native-material/core": "^1.3.7",
    "expo": "~49.0.8",
    "expo-font": "~11.4.0",
    "expo-linking": "~5.0.2",
    "expo-router": "2.0.0",
    "expo-status-bar": "~1.6.0",
    "expo-web-browser": "~12.3.2",
    "moti": "^0.26.0",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-reanimated": "~3.3.0",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-web": "~0.19.6",
    "storybook": "^7.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-export-namespace-from": "^7.22.11",
    "@eslint/create-config": "0.4.6",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native-community/datetimepicker": "7.2.0",
    "@react-native-community/slider": "4.4.2",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-console": "^2.0.0",
